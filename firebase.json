{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/(.*).(js|css|png|jpg|jpeg|gif|svg|ico)",
        "destination": "/$1.$2"
      }
    ]
  }
}